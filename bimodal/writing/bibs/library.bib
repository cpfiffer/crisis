	keywords = {unread, Quantitative Finance - Statistical Finance, Quantitative Finance - Trading and Market Microstructure},
	annote = {Comment: 31 pages, 30 figures, accepted to Quantitative Finance; slightly modified text, a changed figure},
	file = {arXiv.org Snapshot:/home/cameron/Zotero/storage/KSZM9RBX/0904.html:text/html;arXiv\:0904.0900 PDF:/home/cameron/Zotero/storage/4653CSJR/Eisler et al. - 2009 - The price impact of order book events market orde.pdf:application/pdf},
}

@techreport{machain_price_2013,
