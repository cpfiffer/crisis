	author = {Yang, Steve Y. and Qiao, Qifeng and Beling, Peter A. and Scherer, William T. and Kirilenko, Andrei A.},
	month = oct,
	year = {2015},
	keywords = {unread, High-frequency trading, Algorithmic trading, Behavioural finance, Gaussian process, Inverse reinforcement learning, inverse-reinforcement-learning, Markov decision process, Support vector machine},
