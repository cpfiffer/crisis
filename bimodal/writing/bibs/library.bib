	journal = {Econometrica},
	author = {Vives, Xavier},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA8126},
	keywords = {double auction, adverse selection, welfare, collusion, competitiveness, demand schedule competition, market power, public information, rational expectations, Reverse auction},
